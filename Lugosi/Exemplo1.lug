main{
    var int n;
    var int p;
    var int s;

    n := 5;
    p := 3;

    s := potencia(n, p);

    print(s);
}

function int potencia (int n, int p) {
    var int i;
    var int s;

    i := 1;
    s := n;
    
    while ((i < p)) {
        s := (s * n);
        i := (i + 1);
    };

    return s;
}